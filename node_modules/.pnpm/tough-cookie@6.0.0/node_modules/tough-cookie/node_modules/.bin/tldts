#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../tldts@7.0.16/node_modules/tldts/bin/cli.js" "$@"
else
  exec node  "$basedir/../../../../../tldts@7.0.16/node_modules/tldts/bin/cli.js" "$@"
fi

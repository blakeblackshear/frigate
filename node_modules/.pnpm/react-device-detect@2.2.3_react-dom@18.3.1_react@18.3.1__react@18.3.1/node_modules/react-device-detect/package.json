    "enzyme-adapter-react-16": "^1.0.1",
    "jest": "^27.2.2",
    "raf": "^3.4.0",
    "react-test-renderer": "^16.0.0",
    "rollup": "^1.17.0",
    "rollup-plugin-babel": "^4.3.3",

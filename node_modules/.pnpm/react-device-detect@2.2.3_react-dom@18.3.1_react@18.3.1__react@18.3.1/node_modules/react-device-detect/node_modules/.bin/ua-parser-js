#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../ua-parser-js@1.0.41/node_modules/ua-parser-js/script/cli.js" "$@"
else
  exec node  "$basedir/../../../../../ua-parser-js@1.0.41/node_modules/ua-parser-js/script/cli.js" "$@"
fi

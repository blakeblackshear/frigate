#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../terser@5.44.0/node_modules/terser/bin/terser" "$@"
else
  exec node  "$basedir/../../../../../terser@5.44.0/node_modules/terser/bin/terser" "$@"
fi

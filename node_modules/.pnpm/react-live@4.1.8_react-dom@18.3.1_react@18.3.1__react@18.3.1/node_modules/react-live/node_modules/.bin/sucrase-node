#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../sucrase@3.35.0/node_modules/sucrase/bin/sucrase-node" "$@"
else
  exec node  "$basedir/../../../../../sucrase@3.35.0/node_modules/sucrase/bin/sucrase-node" "$@"
fi

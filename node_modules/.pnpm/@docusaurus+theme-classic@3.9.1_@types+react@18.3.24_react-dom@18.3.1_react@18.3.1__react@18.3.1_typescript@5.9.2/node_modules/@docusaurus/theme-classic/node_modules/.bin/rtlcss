#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../rtlcss@4.3.0/node_modules/rtlcss/bin/rtlcss.js" "$@"
else
  exec node  "$basedir/../../../../../../rtlcss@4.3.0/node_modules/rtlcss/bin/rtlcss.js" "$@"
fi

#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../katex@0.16.22/node_modules/katex/cli.js" "$@"
else
  exec node  "$basedir/../../../../../katex@0.16.22/node_modules/katex/cli.js" "$@"
fi

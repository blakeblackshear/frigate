#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../marked@16.3.0/node_modules/marked/bin/marked.js" "$@"
else
  exec node  "$basedir/../../../../../marked@16.3.0/node_modules/marked/bin/marked.js" "$@"
fi

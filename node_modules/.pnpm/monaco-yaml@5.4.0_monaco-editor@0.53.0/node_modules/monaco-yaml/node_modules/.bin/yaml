#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../yaml@2.8.1/node_modules/yaml/bin.mjs" "$@"
else
  exec node  "$basedir/../../../../../yaml@2.8.1/node_modules/yaml/bin.mjs" "$@"
fi

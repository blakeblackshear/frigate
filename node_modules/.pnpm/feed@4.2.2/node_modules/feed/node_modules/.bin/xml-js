#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../xml-js@1.6.11/node_modules/xml-js/bin/cli.js" "$@"
else
  exec node  "$basedir/../../../../../xml-js@1.6.11/node_modules/xml-js/bin/cli.js" "$@"
fi

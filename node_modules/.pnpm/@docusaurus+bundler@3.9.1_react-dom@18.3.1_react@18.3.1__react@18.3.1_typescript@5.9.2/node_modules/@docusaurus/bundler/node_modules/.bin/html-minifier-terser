#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../html-minifier-terser@7.2.0/node_modules/html-minifier-terser/cli.js" "$@"
else
  exec node  "$basedir/../../../../../../html-minifier-terser@7.2.0/node_modules/html-minifier-terser/cli.js" "$@"
fi

#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../uvu@0.5.6/node_modules/uvu/bin.js" "$@"
else
  exec node  "$basedir/../../../../../uvu@0.5.6/node_modules/uvu/bin.js" "$@"
fi

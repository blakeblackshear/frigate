#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../d3-dsv@3.0.1/node_modules/d3-dsv/bin/dsv2dsv.js" "$@"
else
  exec node  "$basedir/../../../../../d3-dsv@3.0.1/node_modules/d3-dsv/bin/dsv2dsv.js" "$@"
fi

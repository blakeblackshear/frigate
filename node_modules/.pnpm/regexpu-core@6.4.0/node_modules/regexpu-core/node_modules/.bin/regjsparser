#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../regjsparser@0.13.0/node_modules/regjsparser/bin/parser" "$@"
else
  exec node  "$basedir/../../../../../regjsparser@0.13.0/node_modules/regjsparser/bin/parser" "$@"
fi

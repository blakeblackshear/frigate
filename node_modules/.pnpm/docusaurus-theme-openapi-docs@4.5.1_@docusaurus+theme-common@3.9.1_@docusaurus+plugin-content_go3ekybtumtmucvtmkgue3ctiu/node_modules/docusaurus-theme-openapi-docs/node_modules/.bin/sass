#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../sass@1.93.2/node_modules/sass/sass.js" "$@"
else
  exec node  "$basedir/../../../../../sass@1.93.2/node_modules/sass/sass.js" "$@"
fi

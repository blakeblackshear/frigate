#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../sitemap@7.1.2/node_modules/sitemap/dist/cli.js" "$@"
else
  exec node  "$basedir/../../../../../../sitemap@7.1.2/node_modules/sitemap/dist/cli.js" "$@"
fi

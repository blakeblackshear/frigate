#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../astring@1.9.0/node_modules/astring/bin/astring" "$@"
else
  exec node  "$basedir/../../../../../astring@1.9.0/node_modules/astring/bin/astring" "$@"
fi

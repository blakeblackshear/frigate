#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../update-browserslist-db@1.1.3_browserslist@4.26.2/node_modules/update-browserslist-db/cli.js" "$@"
else
  exec node  "$basedir/../../../../../update-browserslist-db@1.1.3_browserslist@4.26.2/node_modules/update-browserslist-db/cli.js" "$@"
fi

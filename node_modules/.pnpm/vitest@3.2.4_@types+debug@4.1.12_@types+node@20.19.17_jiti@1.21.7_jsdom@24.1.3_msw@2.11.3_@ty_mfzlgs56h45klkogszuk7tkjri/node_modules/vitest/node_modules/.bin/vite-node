#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../vite-node@3.2.4_@types+node@20.19.17_jiti@1.21.7_sass@1.93.2_terser@5.44.0_yaml@2.8.1/node_modules/vite-node/vite-node.mjs" "$@"
else
  exec node  "$basedir/../../../../../vite-node@3.2.4_@types+node@20.19.17_jiti@1.21.7_sass@1.93.2_terser@5.44.0_yaml@2.8.1/node_modules/vite-node/vite-node.mjs" "$@"
fi

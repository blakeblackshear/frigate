{
  "comments": false,
  "presets": [
    ["@babel/preset-env", {
      "loose": true
    }],
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-transform-runtime",
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-transform-object-assign",
    "dynamic-import-node",
    "react-loadable/babel",
    ["module-resolver", {
      "alias": {
        "react-loadable-ssr-addon": "./source/index.js"
      }
    }]
  ],
  "env": {
    "production": {
      "plugins": [
        ["transform-remove-console", {
          "exclude": [ "error", "warn"]
        }]
      ]
    }
  }
}

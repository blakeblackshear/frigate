#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../openapi-to-postmanv2@4.25.0/node_modules/openapi-to-postmanv2/bin/openapi2postmanv2.js" "$@"
else
  exec node  "$basedir/../../../../../openapi-to-postmanv2@4.25.0/node_modules/openapi-to-postmanv2/bin/openapi2postmanv2.js" "$@"
fi

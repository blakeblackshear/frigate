#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../swagger2openapi@7.0.8/node_modules/swagger2openapi/boast.js" "$@"
else
  exec node  "$basedir/../../../../../swagger2openapi@7.0.8/node_modules/swagger2openapi/boast.js" "$@"
fi

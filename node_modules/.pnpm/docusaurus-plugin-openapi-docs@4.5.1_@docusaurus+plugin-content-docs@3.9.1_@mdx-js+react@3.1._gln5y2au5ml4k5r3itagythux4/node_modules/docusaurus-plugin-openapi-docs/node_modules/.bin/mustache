#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../mustache@4.2.0/node_modules/mustache/bin/mustache" "$@"
else
  exec node  "$basedir/../../../../../mustache@4.2.0/node_modules/mustache/bin/mustache" "$@"
fi

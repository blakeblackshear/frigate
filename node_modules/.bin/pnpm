#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/pnpm@9.15.9/node_modules/pnpm/bin/pnpm.cjs" "$@"
else
  exec node  "$basedir/../.pnpm/pnpm@9.15.9/node_modules/pnpm/bin/pnpm.cjs" "$@"
fi

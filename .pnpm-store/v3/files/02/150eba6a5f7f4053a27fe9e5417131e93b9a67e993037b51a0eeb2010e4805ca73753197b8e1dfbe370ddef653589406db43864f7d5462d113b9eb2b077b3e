        - $ref: '#/components/parameters/collectionFilter'
        - $ref: '#/components/parameters/collectionQuery'
        - $ref: '#/components/parameters/collectionExpand'
        - $ref: '#/components/parameters/collectionFields'
        - name: sort
          in: query
          description: >-
            The collection items sort field and order (prefix with "-" for
            descending sort).
          style: form
          explode: false
          schema:
            type: array
            items:
              type: string
              enum:
                - id
                - '-id'
                - email
                - '-email'
                - averageValue
                - '-averageValue'
                - paymentCount
                - '-paymentCount'
                - lastPaymentTime
                - '-lastPaymentTime'
                - lifetimeRevenue
                - '-lifetimeRevenue'
                - invoiceCount
                - '-invoiceCount'
                - createdTime
                - '-createdTime'
                - updatedTime
                - '-updatedTime'

    GatewayAccountPickInstruction:
      type: object
      discriminator:
        propertyName: method
        mapping:
          gateway-account-weights: '#/components/schemas/gateway-account-weights'
          gateway-acquirer-weights: '#/components/schemas/gateway-acquirer-weights'

    Bind:
      type: object
      description: The rule
      properties:
        name:
          type: string
        labels:
          type: array
          uniqueItems: true
          example:
            - test-rule
            - 'category:foo'
          items:
            type: string
            pattern: '[a-zA-Z][a-zA-Z0-9:-]*'
        status:
          $ref: '#/components/schemas/OnOff'
        criteria:
          $ref: '#/components/schemas/Condition'
        actions:
          type: array
          uniqueItems: true
          items:
            $ref: '#/components/schemas/RuleAction'
      required:
        - name
        - actions
    Rule:
      type: object
      allOf:

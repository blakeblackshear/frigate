    PlanBillingTiming:
      type: string
      default: prepaid
      description: >
        The billing timing in relation to the service period.

        For `prepaid` plans the customer pays when the service period starts,

        whereas, for `postpaid` plans, the customer pays when the service period
        ends.
      enum:
        - prepaid
        - postpaid
